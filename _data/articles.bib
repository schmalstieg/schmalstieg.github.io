@article{Schmalstieg_026,
 author = {Dieter Schmalstieg and Gernot Schaufler},
 title = {Sewing Virtual Worlds Together With SEAMS: A Mechanism to Construct
	Complex Virtual Environments},
 journal = {PRESENCE - Teleoperators and Virtual Environments},
 year= {1999},
 volume = {8},
 pages = {449-461},
 number = {4},
 publisher = {MIT Press},
 url = {http://arbook.icg.tugraz.at/schmalstieg/Schmalstieg_026.pdf},
 selected={1},
 abstract={This paper introduces the Spatially Extended Anchoring Mechanism (SEAM) as a 3-D user-interface metaphor to connect virtual worlds and manage scalability in distributed virtual environments. SEAMs provide a visual and navigable connection between worlds to manage both the complexity of rendering and network communication typically occurring in such environments. In the context of augmented reality, SEAMs can be applied as a 3-D window interface. A rendering algorithm is described which performs well on the graphics accelerators of standard personal computers.},
}

@article{Schmalstieg_027,
 author = {Peter Wonka and Dieter Schmalstieg},
 title = {Occluder Shadows for Fast Walkthroughs of Urban Environments},
 journal = {Computer Graphics Forum},
 year= {1999},
 volume = {18},
 pages = {51-60},
 number = {3},
 month = {September},
 url = {http://arbook.icg.tugraz.at/schmalstieg/Schmalstieg_027.pdf},
 selected={1},
 abstract={This paper describes a new algorithm that employs image-based rendering for fast occlusion culling in complex urban environments. It exploits graphics hardware to render and automatically combine a relatively large set of occluders. The algorithm is fast to calculate and therefore also useful for scenes of moderate complexity and walkthroughs with over 20 frames per second. Occlusion is calculated dynamically and does not rely on any visibility precalculation or occluder preselection. Speed-ups of one order of magnitude can be obtained.},
}


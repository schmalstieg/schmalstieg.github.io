@inproceedings{Schmalstieg_039,
 author = {Dieter Schmalstieg and Anton Fuhrmann and Gerd Hesina},
 title = {Bridging Multiple User Interface Dimensions with Augmented Reality},
 booktitle = {Proceedings of the 3rd International Symposium on Augmented Reality
	(ISAR'00)},
 year= {2000},
 pages = {20-29},
 address = {Munich, Germany},
 month = {October},
 url = {http://arbook.icg.tugraz.at/schmalstieg/Schmalstieg_039.pdf},
 selected={1},
 video={https://www.youtube.com/watch?v=XAWQAdf2CbY},
 abstract={Studierstube is an experimental user interface system,which uses collaborative augmented reality to incorporate true 3D interaction into a productivity environment. This concept is extended to bridge multiple user interface dimensions by including multiple users, multiple host platforms, multiple display types, multiple concurrent applications, and a multi-context (i. e., 3D document) interface into a heterogeneous distributed environment. With this architecture, we can explore the user interface design space between pure augmented reality and the popular ubiquitous computing paradigm. We report on our design philosophy centered around the notion of contexts and locales, as well as the underlying software and hardware architecture. Contexts encapsulate a live application together with 3D (visual) and other data, while locales are used to organize geometric reference systems. By separating geometric relationships (locales) from semantic relationships (contexts), we achieve a great amount of flexibility in the configuration of displays. To illustrate our claims, we present several applications including a cinematographic design tool which showcases many features of our system.},
}

@article{Schmalstieg_040,
 author = {Hannes Kaufmann and Dieter Schmalstieg and Michael Wagner},
 title = {Construct3D: A Virtual Reality Application for Mathematics and Geometry
	Education},
